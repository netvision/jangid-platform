nk to=\"/highlights\" class=\"hover:text-primary\">Highlights</NuxtLink>\n        <NuxtLink to=\"/pricing\" class=\"hover:text-primary\">Pricing</NuxtLink>\n        <template v-if=\"isAuthenticated\">\n          <NuxtLink\n            v-if=\"role === 'SUPER_ADMIN'\"\n            to=\"/dashboard/superadmin\"\n            class=\"rounded-md border border-primary/40 px-3 py-1.5 text-primary shadow-sm hover:bg-primary/5\"\n          >\n            Super admin\n          </NuxtLink>\n          <NuxtLink\n            to=\"/dashboard\"\n            class=\"rounded-md border border-primary/40 px-3 py-1.5 text-primary shadow-sm hover:bg-primary/5\"\n          >\n            Dashboard\n          </NuxtLink>\n          <button\n            type=\"button\"\n            class=\"rounded-md bg-primary px-3 py-1.5 text-white shadow hover:bg-primary/90\"\n            @click=\"handleLogout\"\n          >\n            Logout\n          </button>\n        </template>\n        <NuxtLink\n          v-else\n          to=\"/auth/login\"\n          class=\"rounded-md bg-primary px-3 py-1.5 text-white shadow hover:bg-primary/90\"\n        >\n          Login\n        </NuxtLink>\n      </nav>\n    </div>\n  </header>\n</template>\n\n<script setup lang=\"ts\">\nimport { storeToRefs } from 'pinia'\nimport { useRouter } from 'vue-router'\nimport { useAuthStore } from '@/stores/auth'\n\nconst auth = useAuthStore()\nconst { isAuthenticated, role } = storeToRefs(auth)\nconst router = useRouter()\n\nconst handleLogout = async () => {\n  await auth.logout()\n  router.push('/auth/login')\n}\n</script>\n","<template>\n  <div class=\"min-h-screen bg-surface text-foreground\">\n    <HeaderNav />\n    <main class=\"mx-auto w-full max-w-7xl px-4 py-8\">\n      <slot />\n    </main>\n    <footer class=\"border-t border-border bg-background py-6\">\n      <div class=\"mx-auto flex max-w-7xl flex-col gap-2 px-4 text-sm text-muted-foreground md:flex-row md:items-center md:justify-between\">\n        <p>&copy; 